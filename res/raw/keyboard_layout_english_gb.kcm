# British QWERTY keyboard layout
# UTF-8 encoded
# Nick Lamb - 2012-12-23
# M.Meisser - 2013-10-26
type OVERLAY
map key 43 POUND
key 2 {
	label:	'2'
	base:	'2'
	shift:	'"'
}
key 3 {
	label:	'3'
	base:	'3'
	shift:	'\u00a3'
}
key 4 {
	label:	'4'
	base:	'4'
	shift:	'$'
	ralt:	'\u20ac'
}
key APOSTROPHE {
	label:	'\''
	base:	'\''
	shift:	'@'
}
key POUND {
	label:	'#'
	base:	'#'
	shift:	'~'
	ralt:	'\\'
	shift+ralt:	'|'
}
key GRAVE {
	label:	'`'
	base:	'`'
	shift:	'\u00ac'
	ralt:	'|'
}
